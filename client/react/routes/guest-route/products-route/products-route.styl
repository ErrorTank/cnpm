.products-route {
  white-space nowrap
  font-size 14px
  min-height 300px
  background-color white
  position relative
  color #333
  padding 0
  .left-panel {
    width 240px
    vertical-align top
    display inline-block
    border-radius 4px 0 0 4px
    min-height 300px
  }

  .right-panel {
    .addition-render{
      .provider-info{
        margin-bottom 60px
        .pi-header{
          margin-bottom 30px
          .provider-name{
            font-size 20px
            font-weight bold
            color tikiblack
          }
        }
        .pi-body{
          .info{
            font-size 14px
            color tikiblack
            margin-bottom 10px !important
            span{
              display inline-block
            }
            .icon{
              margin-right 5px
              width 30px
              i{
                font-size 22px
                color tikiblue

              }
            }
            .i-title{
              font-size 14px
            }
            .i-value{
              margin-left 6px
              font-weight bold
            }

          }
        }
      }
      .brand-logo{
        height 50px
        display inline-block
        vertical-align middle
      }
      .bl-contain{
        padding-right 10px
        border-right 2px solid grey
        margin-right 10px
      }

    }

    .main-section{
      position relative
      min-height 600px
      .loading-inline{
        i{
          font-size 50px !important
          color tikiblue
        }
      }
    }
    .visited{
      margin-top 50px
      padding 12px 15px
      position relative
    }
    width calc(100% - 240px)
    vertical-align top
    min-height 300px
    display inline-block
    position relative
    padding-bottom 50px
    border-left 1px solid grey
    border-radius 0 4px 4px 0
    .rp-header{
      padding 20px 15px 5px
      font-size 24px
      .result{
        color #898989
        font-size 23px
      }
    }
  }
}